#!/bin/sh

# Note: LightDM passes the name of the session as the first parameter of .xprofile.
# Other display managers may not, or may source different files.

if [ "xmonad" == $1 ]; then
    numlockx on &
    ~/.fehbg &
    synapse -s &
	dunst & 
    compton --config ~/.xmonad/compton.conf &
    udiskie &
fi
